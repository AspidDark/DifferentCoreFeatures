@using  DPSWeb.DateSelector
@model DPSWeb.Models.ReportModel

<title>@Model.title</title>
<h2>РЕЕСТР НАЧИСЛЕННЫХ %</h2>
<form method="post" enctype="multipart/form-data">
    <div class="form-group row">
        <label asp-for="year" class="col-sm-2 col-form-label">Год</label>
        <div class="col-sm-10">
            <select asp-for="year" class="custom-select mr-sm-2" asp-items="@Model.year">
            </select>
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="month" class="col-sm-2 col-form-label">Месяц</label>
        <div class="col-sm-10">
            <select asp-for="month" class="custom-select mr-sm-2" asp-items="Html.GetEnumSelectList<Month>()">
            </select>
        </div>
    </div>
    <input type="submit" value="Загрузить" />
</form>
